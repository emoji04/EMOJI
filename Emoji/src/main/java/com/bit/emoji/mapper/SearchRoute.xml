<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SearchRoute">

<select id="selectRoute" resultType="com.bit.emoji.model.RouteVO">
select * from emoji.route;
</select>

<select id="selectRouteById" resultType="com.bit.emoji.model.RouteVO" >
select * from emoji.route where routeNum=#{routeNum};
</select>

<select id="selectRouteDeclicious" resultType="com.bit.emoji.model.OrderedPin" >
select * from emoji.routeDelicious where routeNum=#{routeNum};
</select>

<select id="selectJoin" resultType="java.lang.String" parameterType="com.bit.emoji.model.RouteScrapVO">
select joinstate from emoji.join where routeNum=#{routeNum} and memberNum=#{memberNum};
</select>

</mapper>

