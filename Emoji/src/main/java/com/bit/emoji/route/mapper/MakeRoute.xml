<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="MakeRoute">

<select id="selectDelicious" resultType="com.bit.emoji.model.DeliciousPinVO" parameterType="String">
select * from delicious where deliciousPinDetail like '%' || #{search} || '%'
</select>




</mapper>

